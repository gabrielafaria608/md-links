{
    "env": {
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "script"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "no-console": "off",
        "import/extensions": "off",
        "no-shadow": "off",
        "no-plusplus":"off"
    },
    "plugins": ["jest"]
}
